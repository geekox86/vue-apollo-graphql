{
  "name": "vue-apollo-graphql",
  "version": "0.0.1",
  "description": "Vue integration of Apollo GraphQL",
  "main": "builds/index.js",
  "types": "builds/index.d.ts",
  "author": "Mohannad Al-Awad",
  "license": "MIT",
  "scripts": {
    "test": "yarn jest --watch",
    "debug": "",
    "build": "",
    "release": ""
  },
  "dependencies": {
    "apollo-client": "^2.5.1",
    "vue": "^2.6.10"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/fibers": "^0.0.30",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "24.0.11",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^11.13.5",
    "@types/sass": "^1.16.0",
    "@types/webpack": "^4.4.26",
    "@types/webpack-dev-server": "^3.1.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "cssnano": "^4.1.10",
    "fibers": "^3.1.1",
    "file-loader": "^3.0.1",
    "fusebox": "^1.3.123",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jest-chain": "^1.1.2",
    "jest-extended": "^0.11.1",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "sass": "^1.19.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.4",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {},
      "cssnano": {}
    }
  },
  "jest": {
    "testURL": "http://localhost:8080/",
    "testMatch": [
      "<rootDir>/tests/**/*.(js|jsx|ts|tsx|vue)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.(aac|m4a|mp3|mp4|oga|wav|webm|gif|jpg|jpeg|png|svg|eot|otf|ttf|woff|woff2|css|sass|scss)$": "jest-transform-stub",
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "snapshotResolver": "<rootDir>/tests/snapshots/resolver.js",
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "setupFilesAfterEnv": [
      "jest-chain",
      "jest-extended"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  }
}
